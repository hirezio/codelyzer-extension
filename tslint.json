{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-empty": false,
        "max-classes-per-file": false,
        "ordered-imports": false,
        "trailing-comma": false,
        "no-console": false,
        "arrow-parens": false,
        "no-var-requires": false,
        "interface-name": false,
        "object-literal-sort-keys": false,
        "quotemark": [
            true,
            "single"
        ]
    },
    "rulesDirectory": []
}